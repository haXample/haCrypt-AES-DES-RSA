    ICON            "haCryptSymbol", IDC_STATIC, 7, 10, 20, 20     // Icon position & size

    GROUPBOX        "Crypto algorithm development support - Verification of given data samples",
    IDC_STATIC, 37, 7, 320, 105 // Frame position & size
 
    LTEXT " Crypto specs often provide a 'Known Answer Test (KAT)'\n \
with binary data sets given as AscHex text.\n\n \
KAT:\n \
- Crypto: Choose an algo mode\n \
- KAT: Paste Key, IV, Plaintext into the Text-field\n \
- AscHex2Bin: Apply the data successively\n \
- Text-field: Result"

    IDC_TEST, 40, 40, 300, 70                                      // Help field position & size
END

////////////////////////////////////////////////////////////////////////////////
//
//                       Main menu: File
//                                      
// Note: The "&-prefix" defines a letter that will be underlined when 
// pressing 'Alt' on the keyboard. Alt+Letter alternatively invokes the Menu.
//
IDR_MAINMENU MENU DISCARDABLE           // Main menu
BEGIN                                   
    POPUP "&File"                       // 0: Main menu 'File'
    BEGIN
        MENUITEM "&New",              ID_FILE_TEXT_NEW
        MENUITEM "&Open...",          ID_FILE_TEXT_OPEN
        MENUITEM "&Save as...",       ID_FILE_TEXT_SAVEAS, GRAYED  // Initially disabled ('grayed')
        MENUITEM SEPARATOR
        MENUITEM "&Rename file type", ID_FILE_TEXT_RENAME
        MENUITEM SEPARATOR
        MENUITEM "E&xit",             ID_FILE_EXIT
    END // &File

////////////////////////////////////////////////////////////////////////////////
//
//                       Main menu: Crypto
//                                      
    POPUP "&Crypto"                     // 1: Main menu 'Crypto'
    BEGIN
        POPUP "&DES"                    // [0]: Submenu DES
        BEGIN
            MENUITEM "DES /Encrypt (CBC Ciphertext stealing)",      ID_CRYPTO_DES_ENCRYPT 
